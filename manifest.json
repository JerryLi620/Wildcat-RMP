{
  "name": "Wildcat RMP",
  "version": "1.0.0",
  "description": "embeded rate my professor chrome extension to Davidson College course schedule page",
  "author": "Mingyang Li",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "wildcat.png",
    "48": "wildcat.png",
    "128": "wildcat.png"
  },
  "action": {
    "default_title": "Wildcat RMP",
    "default_popup": "popup.html"
  }
}
